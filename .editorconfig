[*]
charset=utf-8
end_of_line=lf
insert_final_newline=false
indent_style=space
indent_size=4

[*.json]
indent_style=space
indent_size=2
end_of_line=lf
trim_trailing_whitespace=true
insert_final_newline=true

[{*.ts,*.js,.eslintrc.cjs}]
indent_style=tab
tab_width=4
end_of_line=lf
trim_trailing_whitespace=true
insert_final_newline=true